<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#FF1E1E1E</Color>
    <Color x:Key="ForegroundColor">#FFFFFF</Color>
    <Color x:Key="TextboxBackground">#2A2A2A</Color>
    <Color x:Key="TextboxBorder">#444444</Color>

    <SolidColorBrush x:Key="WindowBackground" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="WindowForeground" Color="White"/>
    <SolidColorBrush x:Key="TextboxBrush" Color="{StaticResource TextboxBackground}"/>
    <SolidColorBrush x:Key="BorderBrushColor" Color="{StaticResource TextboxBorder}"/>
    

</ResourceDictionary>

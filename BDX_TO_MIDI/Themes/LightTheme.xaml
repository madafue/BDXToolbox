<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#FFF9F9F9</Color>
    <Color x:Key="ForegroundColor">#000000</Color>
    <Color x:Key="TextboxBackground">#FFFFFF</Color>
    <Color x:Key="TextboxBorder">#CCCCCC</Color>

    <SolidColorBrush x:Key="WindowBackground" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="WindowForeground" Color="{StaticResource ForegroundColor}"/>
    <SolidColorBrush x:Key="TextboxBrush" Color="{StaticResource TextboxBackground}"/>
    <SolidColorBrush x:Key="BorderBrushColor" Color="{StaticResource TextboxBorder}"/>

</ResourceDictionary>
